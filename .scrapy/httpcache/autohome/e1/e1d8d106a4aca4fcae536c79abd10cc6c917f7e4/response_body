		var len_url_stats =url_stats.length;
		}
})();

  function func_stats(){
      var image = new Image(1, 1);
      image.onload = image.onerror = function() {
      image.onload =  image.onerror = null;
      image = null;
      };
      image.src = url_stats;
  }
