    <script type="text/javascript">
        seajs.config({
            version: 20181029,
            alias: {
                'JsonpExtend': '/static/scripts/javascript/JsonpExtend',
                'support': '//s.autoimg.cn/as/as-3.1.3/support.js',
                'toolCookie': '//s.autoimg.cn/car/sidebar/common/js/tool-cookie.js',
                'toolbar': '//s.autoimg.cn/car/sidebar/common/js/toolbar-1.1.0.js?v=20181029',
                'staytime': '//s.autoimg.cn/car/duibi_new/staytime.js',

            },
            paths: {
                'as': '//x.autoimg.cn/as/as-3.1.2'
            }
