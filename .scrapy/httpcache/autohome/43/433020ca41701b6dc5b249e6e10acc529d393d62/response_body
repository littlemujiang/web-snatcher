                                    if (item.price > 0) {
                                        allowanceIsShow = true;
                                    }
                                    items.push({
                                        "specid": item.specid,
                                        "price": item.price

                                    });
                                });
                                allowancePrices.result.list = items;
                                loadLock = true;
                            }
                        });
            }
            if (seriesid == 3195) {
                //车商城一口价
