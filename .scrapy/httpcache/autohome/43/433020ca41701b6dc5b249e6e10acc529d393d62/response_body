                });
                staytime.initConfig();
            });


            var showAllowance = false;
                $("#navScrollLeft li").each(function () {
                if ($(this).text().indexOf('电动机') != -1) {
                    showAllowance = true;
                    return false;
                }
            });
            var loadLock = false;
            //补贴后售价
            if (showAllowance) {
                $.
                getJSONP('//carif.api.autohome.com.cn/car/getspecelectricbutie.ashx',
                        {
                            speclist: specIDs.join(),
