                    miniBtnShow: true//mini按钮默认显示
                });
                staytime.initConfig();
            });


            var showAllowance = false;
                $("#navScrollLeft li").each(function () {
                if ($(this).text().indexOf('电动机') != -1) {
                    showAllowance = true;
                    return false;
                }
            });
            var loadLock = false;
            //补贴后售价
            if (showAllowance) {
                $.
                getJSONP('//carif.api.autohome.com.cn/car/getspecelectricbutie.ashx',
                        {
                            speclist: specIDs.join(),
                            cityid: GetUserCityId(),
                            jsonp: "_callback",
                            jsonpCallback: "GetSpecElectricSubsidy"
                        }, function (jsonData) {
                            allowancePrices = {"returncode": 0, "result": {"list": []}};
                            if ( jsonData && jsonData.returncode == 0 && jsonData.result && jsonData.result.specitems) {
                                var items = [];
                                    $(jsonData.result.specitems).each(function (i, item) {
                                    if (item.price > 0) {
                                        allowanceIsShow = true;
                                    }
                                    items.push({
                                        "specid": item.specid,
                                        "price": item.price

                                    });
                                });
                                allowancePrices.result.list = items;
                                loadLock = true;
                            }
                        });
            }
            if (seriesid == 3195) {
                //车商城一口价
                var url = '//api.mall.autohome.com.cn/getseriesad/' + seriesid + '/' + GetUserCityId() + '?_appid=car&callback=?';
                $
            .
                getJSON(url, function (json) {
                    if (json != undefined && json != null && json.returncode == 0) {
                        if (json.result.list[0] != undefined) {
                            saleSpecs = json.result.list[0].SpecAds;
                            var a = $('#huang');
                            a.html(json.result.list[0].Title + '<i class="crazed-tips-iconl"></i><i class="crazed-tips-iconr"></i>');
                            a.attr('href', json.result.list[0].Url);
                            a.show();
                            wasLaodSaleSpecs = true;
                        }
                    }
                });
            } else {
                var a = $('#huang');
                a.hide();
            }
            //经销商报价处理
            $
            .
            getJSONP('//carif.api.autohome.com.cn/dealer/LoadDealerPrice.ashx',
                    {
                        type: 1,
                        seriesid: seriesid,
                        jsonp: "_callback",
                        jsonpCallback: "LoadDealerPrice",
                        city: GetUserCityId()
                    },// 即将上市依旧全国不修改
                    function (jsonObj) {
                        dealerPrices = {"returncode": 0, "result": {"list": []}};
                        if (jsonObj.success && jsonObj.body && jsonObj.body.item) {
                            var items = [];
                                $(jsonObj.body.item).each(function (i, item) {
                                items.push({"SpecId": item.SpecId, "Price": item.MinPrice});
                            });

                            dealerPrices.result.list = items;
                            var timerOne = null;
                            clearInterval(timerOne);

                            timerOne = setInterval(function () {
                                if (loadLock) {
                                    clearInterval(timerOne);
                                    _paramConfig.allowancePrice();
                                    _paramConfig.dealerPrice();
                                    var ids = _paramConfig.selectCar().join(',');
                                    _paramConfig.responseContent(ids);
                                    _paramConfig.scrollFloat(true);
                                    _paramConfig.selectEvent();
                                }
                                else {
                                    if (showAllowance == false) {
                                        clearInterval(timerOne);
                                        _paramConfig.dealerPrice();
                                        var ids = _paramConfig.selectCar().join(',');
                                        _paramConfig.responseContent(ids);
                                        _paramConfig.scrollFloat(true);
                                        _paramConfig.selectEvent();
                                    }
                                }
                                if (wasLaodSaleSpecs == true) {
                                    clearInterval(timerOne);
                                        _paramConfig.saleSpecList();

                                        var ids = _paramConfig.selectCar().join(',');
                                        _paramConfig.responseContent(ids);
                                        _paramConfig.scrollFloat(true);
                                        _paramConfig.selectEvent();

                                }
                                else
                                {
                                    clearInterval(timerOne);
                                    _paramConfig.dealerPrice();
                                    var ids = _paramConfig.selectCar().join(',');
                                    _paramConfig.responseContent(ids);
                                    _paramConfig.scrollFloat(true);
                                    _paramConfig.selectEvent();
                                }
                            }, 100);

                        }
                    });

            //下载配置统计
            (function () {
                var configCookie = 'seriesconfigfile';
                    $('#btnDownLoadConfig').on('click', function () {
                        $('body').append('<iframe name="ifmDownLoadConfig" src="/pv/pvareahelp.html#pvareaid=103966" style="display:none;"></iframe>');

                    /*新统计 下载配置 start*/
                    var strParam = "{'eid': '1|1|48|35|200048|300043',";
                    for (var i = 0; i < specIDs.length && i < 9; i++) {
                        strParam += "'a" + (i + 1) + "':'" + specIDs[i] + "',";
                    }
                    strParam += "'a10': '" + (new Date).valueOf() + "'}";
                    _trackEvent.push(strParam);
                    /*新统计 下载配置end*/
                });
            }());

            //统计1次页面是否有选装报价内容
            (function () {
                if ($("#config_data .optional").length > 0) {
                    _trackEvent.push("{'eid':'1|2|572|3286|200130|300171'}");
                }
            })();
        })
    </script>
    
<!--二手车导航置灰-->
<script type="text/javascript">
    seajs.use(["jquery", "/static/scripts/new_script/func-area.js?20160422"],
            function ($, area) {
                $.
                ajax({
                    url: "//api.che168.com/auto/ForAutoCarPCInterval.ashx",
                    data: {
                        _appid: "car",
                        sid: "3661",
                        pid: area.provinceId
                    },
                    jsonpCallback: 'che168CallBack',
                    dataType: 'jsonp',
                    success: function (data) {
                        if (!data.result || data.result.count == 0) {
                                $("#liChe168link").html('<span>二手车</span>');
                        }
                        else {
                            if (area.cityId > 0) {
                                //跳转到相应城市
                                var ahref = $("#nav_che168link").attr("href");
                                    $("#nav_che168link").attr("href", ahref.replace("china", area.cityId));
                            }
                        }
                    },
                    cache: true
                });
            });
</script>

